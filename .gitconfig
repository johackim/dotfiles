[core]
    excludesfile = ~/.gitignore_global
[color]
    ui = true
[alias]
    s = status -sb
    l = log --graph --oneline
    moustache = stash
    cleanup = "!f() { git branch --merged | grep -v '*' | xargs -n 1 git branch -d; }; f"
    undo = reset --soft HEAD^
    prune = fetch --prune
    url = config --get remote.origin.url
[diff]
    tool = icdiff
[difftool]
    prompt = false
[difftool "icdiff"]
    cmd = /usr/bin/icdiff --line-numbers $LOCAL $REMOTE
[commit]
    gpgsign = true
[user]
    name = ston3o
    email = contact@ston3o.me
[gpg]
    program = gpg2
